{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NestJS Backend",
      "program": "${workspaceFolder}/back/node_modules/@nestjs/cli/bin/nest.js",
      "args": ["start"],
      "cwd": "${workspaceFolder}/back",
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": ["${workspaceFolder}/back/dist/**/*.js"]
    }
  ]
}
